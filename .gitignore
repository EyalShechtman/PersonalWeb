# Ignore node_modules directory, which contains dependencies installed via npm/yarn
node_modules/

# Ignore logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore dotenv environment variable files (used for sensitive data like API keys)
.env
.env.local
.env.*.local

# Ignore build directories and output
build/
dist/
out/

# Ignore Next.js cache (if you're using Next.js)
.next/

# Ignore local OS or editor files (e.g., macOS or Windows files)
.DS_Store
Thumbs.db

# Ignore lock files (depending on your package manager; use only one)
# Uncomment the one you're using and ignore the other
# package-lock.json (if using npm)
# yarn.lock (if using yarn)

# Ignore coverage reports
coverage/

# Ignore temporary files generated by editor/IDE
.vscode/
.idea/

# Ignore Git Large File Storage pointer files
*.lfs

# Ignore temporary files like swap files
*.swp
node_modules/
